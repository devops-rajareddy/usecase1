pipeline {
    agent any
    
    stages {
      withCredentials([azureServicePrincipal('azure-sp')])  {
      stage('Init') {
         steps {
           sh 'terraform init'
           
           }
           }
           
          stage('Apply') {
          steps {
          
          sh 'terraform apply'
          }
          }
          }
          
          }
          
          }
